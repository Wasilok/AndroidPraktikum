<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Accelerometer" />


    <TextView
        android:id="@+id/accelerometer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/Barometer" />

    <TextView

        android:id="@+id/Lon_view"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="TextView" />



    <TextView

        android:id="@+id/Lat_view"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="TextView" />



    <TextView

        android:id="@+id/Loc_view"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="TextView" />



    <TextView

        android:id="@+id/Spd_view"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="TextView" />



    <TextView

        android:id="@+id/Alt_view"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="TextView" />



    <TextView

        android:id="@+id/Sat_view"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="TextView" />



    <TextView

        android:id="@+id/Gps_status"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="TextView" />



    <TextView

        android:id="@+id/Update_status"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="TextView" />



    <Button

        android:id="@+id/button"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="Button" />

    <com.google.android.gms.common.SignInButton
        android:id="@+id/sign_in_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="518dp" />

    <Button
        android:id="@+id/button_sign_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="clear"
        android:text="Sign Out"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="597dp" />

    <TextView

        android:id="@+id/tVpersonName"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="Name" />

    <TextView

        android:id="@+id/tVpersonGivenName"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="Vorname" />

    <TextView

        android:id="@+id/tVpersonFamilyName"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="Nachname" />

    <TextView

        android:id="@+id/tVpersonEmail"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"

        android:text="e-Mail" />

    <ImageView
        android:id="@+id/tVpersonPhoto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        tools:srcCompat="@tools:sample/avatars" />

</LinearLayout>